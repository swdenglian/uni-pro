{
  "name": "uni-app-vue3-demo",
  "version": "0.1.0",
  "scripts": {
    "dev:h5": "uni --mode prod",
    "dev:h5:dev": "uni --mode dev",
    "dev:mp-weixin": "uni -p mp-weixin --mode prod",
    "dev:mp-weixin:dev": "uni -p mp-weixin --mode dev",
    "build:h5:dev": "uni build --mode dev",
    "build:h5": "uni build --mode prod",
    "build:mp-weixin": "uni build -p mp-weixin",
    "build:android:dev": "uni build -p app --mode dev && cross-env ENV=.env.dev vite-node ./scripts/build-android.ts",
    "build:android": "uni build -p app --mode prod && cross-env ENV=.env.prod vite-node ./scripts/build-android.ts",
    "build:pages": "vite-node --config vite-node.config.ts ./scripts/build-pages.mts",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:lint-staged": "lint-staged"
  },
  "dependencies": {
    "@antfu/eslint-config": "^3.12.1",
